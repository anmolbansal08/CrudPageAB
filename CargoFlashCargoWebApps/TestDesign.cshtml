<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@Page.Title - My ASP.NET Web Page</title>
    <link type="text/css" rel="stylesheet" href="Styles/Application.css" />
    <link type="text/css" rel="stylesheet" href="Styles/Grid/kendo.common.min.css" />
    <link type="text/css" rel="stylesheet" href="Styles/Grid/kendo.silver.min.css" />
    <link type="text/css" rel="stylesheet" href="Styles/Site.css" />
    <link type="text/css" rel="stylesheet" href="Styles/validator.theme.red.css" />
    <link type="text/css" rel="stylesheet" href="Styles/jquery.appendGrid-1.3.2.css" />
    <link type="text/css" rel="stylesheet" href="Styles/CfiMessage.css" />
    @*
        <link type="text/css" rel="stylesheet" href="Styles/bootstrap.css" />*@
    <script type="text/javascript" src="Scripts/jquery-1.7.2.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery.toolbar.js"></script>
    <script type="text/javascript" src="Scripts/kendo/kendo.web.js"></script>
    <script type="text/javascript" src="~/Scripts/validator.js"></script>
    <script type="text/javascript" src="~/Scripts/Common.js"></script>
    <script src="~/Scripts/CfiMessage.js"></script>
    <script type="text/javascript" src="Scripts/maketrans.js"></script>
    <script src="Scripts/jquery.appendGrid-1.3.2.js"></script>
    <script src="Scripts/jquery.serializeToJSON.js"></script>
    <meta name="viewport" content="width=device-width" />
    
</head>
@{
    var MenuItem = "";
    if (Session["UserDetail"] != null)
    {
        CargoFlash.Cargo.Model.UserLogin userLogin = (CargoFlash.Cargo.Model.UserLogin)Session["UserDetail"];

        Session["LoginID"] = "Admin";
        Session["LoginSNo"] = "1";
        Session["DateFormat"] = "dd-MMM-yyyy";
        MenuItem = userLogin.MenuDetails;
    }
    else
    {
        System.Web.HttpContext.Current.Response.Redirect("~/Account/Login.cshtml");
    }
}

<body >
    <style type="text/css">
        body, html {
            overflow: hidden;
        }
    </style>

    <header id="header">
        <div style="height: 53px; background: #005c83;">
            <span id="logo">
                <img src="~/images/user.png">
            </span><a style="color:#FFFFFF" href="~/Account/Login.cshtml?islogout=true">Logout</a>
        </div>

        <div class="menu-title" style="display:none;">
            @Html.Raw(@MenuItem)
        </div>
    </header>
    <script type="text/javascript">
        $(document).ready(function () {
            if (window.opener)
                userContext = window.opener.parent.userContext;
            $("div").click(function () {
                alert('madhav');
                var content = 'div class="div"';
                $("#destination").wrap(document.createElement(content));
            });
        });
    </script>
    <style>
        .div {
            margin: 10px;
            padding: 12px;
            border: 2px solid #666;
            width: 60px;
        }
    </style>

    <p>Click on any square below to see the result:</p>
    <div class="div" id="destination">THIS IS TEST</div>
    <div class="div" style="background-color:blue;">ONE</div>
    <div class="div" style="background-color:green;">TWO</div>
    <div class="div" style="background-color:red;">THREE</div>



</body>
</html>