@{
    ViewBag.Title = "AWB Rate Audit Log";
}
@model CargoFlash.Cargo.Model.SpaceControl.SpaceControlSearchModel
<script src="~/Scripts/KendoSchedular/Content/JS/kendo.dataviz.min.js"></script>
<br />
<div>
    <div>
        @using (Html.BeginForm("AWBRateAuditLog", "GetData", FormMethod.Post, new { enctype = "multipart/form-data", @id = "SearchAWBRateAuditLogSearchForm" }))
        {
            <table class="WebFormTable">
                <tbody>
                    <tr>
                        <td class="formlabel" style="width: 100px;">
                            <font color="red">*</font>
                            Search By :
                        </td>
                        <td class="formInputcolumn">
                            @Html.RadioButton("SearchBy", 1, true)Reference Number
                            @Html.RadioButton("SearchBy", 2)AWB Number
                        </td>
                     
                        <td  style="width:80px;">
                            <font color="red">*</font>
                            <span id="spnAirlineCode" style="font-weight:bold">Airline Code</span>
                        </td>
                        <td style="width:100px;">
                            @Html.Hidden("AirlineCode")
                            @Html.TextBox("Text_AirlineCode", null, new {placeholder = "Airline Code", controltype = "autocomplete", data_role = "autocomplete" })
                        </td>

                        <td class="formlabel" style="width: 100px;">
                            <font color="red">*</font>
                            <span id="refNo">Reference Number</span>
                        </td>
                        <td class="formInputcolumn">
                            @Html.Hidden("ReferenceNumber")
                            @Html.TextBox("Text_ReferenceNumber", null, new { style = "width:200px", data_valid = "required", @data_valid_msg = "Select the Reference Number", @required = "required", placeholder = "Reference Number", controltype = "autocomplete", data_role = "autocomplete" })
                        </td>
                        <td class="formbuttonrow" colspan="2">
                            <input type="button" tabindex="4" class="btn btn-success" onclick="SearchAWBRateAuditLog(1)" name="Search" style="width:80px;" value="Search">
                        </td>
                    </tr>
                </tbody>
            </table>
        }
    </div>
</div>

<ul class="tabrow">
    <li class="selected">
        <a class="tablinks" onclick="OpenTab(event, 'AWBRateAuditLogSerachGridDivInfo', 1)">AWB Information</a>
    </li>
    <li class="">
        <a class="tablinks" onclick="OpenTab(event, 'AWBRateAuditLogSerachGridDivRateLogHistorytab', 2)">Rate Log</a>
    </li>
    <li class="">
        <a class="tablinks" onclick="OpenTab(event, 'AWBRateAuditLogSerachGridDivOtherChargesHistoryTab', 3)">Other Charges Log</a>
    </li>
    <li class="">
        <a class="tablinks" onclick="OpenTab(event, 'AWBRateAuditLogSerachGridDivTaxRateHistoryTab', 4)">Tax Rate Log</a>
    </li>      
</ul>
