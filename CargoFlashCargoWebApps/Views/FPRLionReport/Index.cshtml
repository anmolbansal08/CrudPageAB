            </td>

            <td class="formlabel"></td>
            <td class="formInputcolumn">

                @*@Html.Hidden("CurrencySNo")
                @Html.TextBox("Text_CurrencySNo", null, new { controltype = "autocomplete", data_role = "autocomplete" })*@

            </td>

        </tr>


        <tr class='formSection'>

            <td class="formlabel"><font color='red'></font>Origin</td>
            <td class="formInputcolumn">

                @Html.Hidden("OriginSNo")
                @Html.TextBox("Text_OriginSNo", null, new { controltype = "autocomplete", data_role = "autocomplete" })

            </td>
            <td class="formlabel">Destination</td>
            <td class="formInputcolumn">

                @Html.Hidden("DestinationSNo")
                @Html.TextBox("Text_DestinationSNo", null, new { controltype = "autocomplete", data_role = "autocomplete" })


            </td>

        </tr>


