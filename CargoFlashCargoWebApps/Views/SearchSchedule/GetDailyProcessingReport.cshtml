<table class='WebFormTable' id='tbl' validateonsubmit=true>
    <tr><td class='formSection' colspan='10'>Daily Processing Report </td></tr>
    <tr><td class='formlabel' colspan='10'><div class='MandatoryTextCss'><font color='red'>*</font> Mandatory Fields</div></td></tr>
    <tr class='formSection'>

        <td class="formlabel"><font color='red'>*</font> Date</td>
        <td class="formInputcolumn">@Html.TextBox("date", null, new { data_valid = "required", data_valid_msg = " Date can not be blank" })</td>
        @*<td class="formlabel">Top </td>
            <td class="formInputcolumn">

                @Html.Hidden("TopNumber")
                @Html.TextBox("Text_TopNumber", null, new {data_valid = "required", data_valid_msg = " Top can not be blank", controltype = "autocomplete", data_role = "autocomplete", style = "width:250px;" })


            </td>*@





    </tr>

    <tr class='formSection'>
        <td colspan='10'>
            <input type="button" name='SearchSave' value='Search' class='btn btn-success' id="btnSubmit" />
            @*<input type="button" name='ExcelVersionDetails' value='Export To Excel' class='btn btn-success' id="btnSubmit" src="../Images/IconExcel.png" />*@
        </td>
    </tr>


