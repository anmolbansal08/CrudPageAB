<table class='WebFormTable' id='tbl' validateonsubmit=true>
    <tr><td class='formSection' colspan='10'>Claim Complaint Tracking </td></tr>
    <tr><td class='formlabel' colspan='10'><div class='MandatoryTextCss'><font color='red'>*</font> Mandatory Fields</div></td></tr>
    <tr class='formSection'>



        <td class="formlabel" title="Select Based On"><font color="red">*</font><span id="spnBasedOn"> Based On</span></td>
        <td class="formInputcolumn">
            <input type="radio" tabindex="18" data-radioval="Yes" class="" name="BasedOn" checked="True" id="BasedOn" value="0">Claim No.
            <input type="radio" tabindex="18" data-radioval="No" class="" name="BasedOn" id="BasedOn" value="1">Complaint No.
        </td>

        <td class="formlabel"><font color='red'>*</font>Claim/Complaint Ref No.</td>
        <td class="formInputcolumn">

            @Html.Hidden("ClaimComplaintNo")
            @Html.TextBox("Text_ClaimComplaintNo", null, new { data_valid = "required", controltype = "autocomplete", data_role = "autocomplete", data_valid_msg = "Select Claim/Complaint No." })

        </td>
        @*<td class="formlabel"><font color='red'></font> </td>
        <td class="formInputcolumn"></td>*@

    </tr>
    @*<tr class='formSection'>

        <td class="formlabel"><font color='red'>*</font>From Date</td>
