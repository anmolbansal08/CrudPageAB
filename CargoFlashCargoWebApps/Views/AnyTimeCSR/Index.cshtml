<style type="text/css">
    .k-grid-header th.k-header > .k-link {
        cursor: auto;
    }
</style>


<div id="divbody" style="height:auto">
    <table class='WebFormTable' id='tbl' validateonsubmit=true>
        <tr><td class='formSection' colspan='4'>AnyTime CSR Report Information</td></tr>
        <tr class="DateWiseRow">
            <td class=' formlabel' title='Airline'>
                <font color='red'>*</font><span id='spnAirlineCode'>
                    Airline :
                </span>
            </td>
            <td class='formInputcolumn'>
                @Html.Hidden("AirlineSNo")
                @Html.TextBox("Text_AirlineSNo", null, new { data_valid = "required", controltype = "autocomplete", data_role = "autocomplete", data_valid_msg = "Airline Name can not be blank", style = "width:160px;" })
                
            </td>

            <td class=' formlabel' title='City'>
                @*<font color='red'>*</font>*@
                <span id='spnAirlineCode'> City : </span>
                
            </td>
            <td class='formInputcolumn'>
                @*@Html.Hidden("CitySNo")
                @Html.TextBox("Text_CitySNo", null, new { data_valid = "required",controltype = "autocomplete", data_role = "autocomplete",data_valid_msg = "City Name can not be blank", style = "width:160px;" })*@

                @Html.Hidden("CitySNo")
                @Html.TextBox("Text_CitySNo", null, new {controltype = "autocomplete", data_role = "autocomplete", style= "width:160px;" })

            </td>

        </tr>

       
        <tr class='formSection'>

            <td class="formlabel">Office :</td>
            <td class="formInputcolumn">

                @Html.Hidden("OfficeSNo")
                @Html.TextBox("Text_OfficeSNo", null, new { controltype = "autocomplete", data_role = "autocomplete", style = "width:160px;" })


            </td>

            <td class="formlabel" title='Agent'>Agent :</td>
            <td class="formInputcolumn">
             
                @Html.Hidden("AccountSNo")
                @Html.TextBox("Text_AccountSNo", null, new { controltype = "autocomplete", data_role = "autocomplete", style = "width:160px;" })
            </td>
                   
        </tr>


        <tr class='formSection'>

            <td class="formlabel">
                @*<font color='red'>*</font>
                <span id='spnAirlineCode'></span>*@
                Currency :

            </td>
            <td class="formInputcolumn">

                @Html.Hidden("CurrencySNo")
                @Html.TextBox("Text_CurrencySNo", null, new { controltype = "autocomplete", data_role = "autocomplete", style = "width:160px;" })

            </td>

            <td class="formlabel"><font color='red'></font>
            
            </td>

            <td class="formInputcolumn">

            </td>
            
               
</tr>
   

        <tr class="DateWiseRow">
            <td class=' formlabel' title='From Date'>
                <font color='red'></font><span id='spnAirlineCode'>
                    From Date :
                </span>
            </td>
            <td class='formInputcolumn'>
                <input type='text' name='FromDate' id='FromDate' style='width: 160px;' data-valid='required'
                       data-role="datepicker" data-valid-msg='Select From Date' controltype='datetype' value='' placeholder='From Date' />
            </td>
            <td class=' formlabel' title='To Date'>
                <font color='red'></font><span id='spnAirlineCode'>
                    To Date :
                </span>
            </td>
            <td class='formInputcolumn'>
                <input type='text' name='ToDate' id='ToDate' style='width: 160px;' data-valid='required'
                       data-role="datepicker" data-valid-msg='Select From To Date' controltype='datetype' value='' placeholder='To Date' />
            </td>
        </tr>
        
        <tr>
            <td class=' formlabel'>
                <div id="exportflight" style="float:left;">
                    <img id="imgexcel" src="../Images/IconExcel.png" style="width:30px;height:30px;cursor: pointer" onclick="ExportExcelHoldType()" title="Export To Excel" />
                </div>
            </td>
            <td class='formInputcolumn'>
                <input type='submit' name='operation' onclick="SearchAnyTimeCSR()" value='Search' id="btnSearch" class='btn btn-success btnSearch' />
                @*<input type='submit' name='operation' onclick="SearchFPRLionReport()" value='Search' id="btnSearch" class='btn btn-success btnSearch' />*@
                @*onclick="DownloadExcelReport()" name='operation'*@
            </td>
            <td class="formlabel"></td>
            <td class="formInputcolumn"></td>

        </tr>
    </table>
    <br />
    <br />
</div>
<table id='tbl'></table>

<div id="example">
    <div id="grid" style="font-size:11px;"></div>
    <script src="~/JScript/Report/AnyTimeCSR.js"></script>

</div>


