<table class='WebFormTable' id='tbl' validateonsubmit=true>
    <tr><td class='formSection' colspan='10'>Cargo Claim Report </td></tr>
    <tr><td class='formlabel' colspan='10'><div class='MandatoryTextCss'><font color='red'>*</font> Mandatory Fields</div></td></tr>
    <tr class='formSection'>

        <td class="formlabel"><font color='red'>*</font>Branch Office</td>
        <td class="formInputcolumn">

            @Html.Hidden("BranchOffice")
            @Html.TextBox("Text_BranchOffice", null, new { data_valid = "required", controltype = "autocomplete", data_role = "autocomplete", data_valid_msg = "Select Branch Office" })

        </td>
        <td class="formlabel"><font color='red'>*</font>Month of Claim </td>
        <td class="formInputcolumn">
            @Html.TextBox("MonthofClaim", null, new { data_valid = "required", controltype = "textbox", data_role = "textbox", data_valid_msg = "Select Month of Claim " })

        </td>

    </tr>
    <tr class='formSection'>

        <td class="formlabel"><font color='red'>*</font>Status Of Claim</td>
        <td class="formInputcolumn">

            @Html.Hidden("StatusOfClaim")
            @Html.TextBox("Text_StatusOfClaim", null, new { data_valid = "required", controltype = "autocomplete", data_role = "autocomplete", data_valid_msg = "Select Status Of Claim" })

        </td>
        <td class="formlabel"> </td>
        <td class="formInputcolumn"></td>

       


    </tr>


    <tr class='formSection'>
        <td colspan='10'>
            <input type="button" name='SearchSave' value='Search' class='btn btn-success' id="btnSubmit" />
            @*<input type="button" name='ExcelVersionDetails' value='Export To Excel' class='btn btn-success' id="btnSubmit" src="../Images/IconExcel.png" />*@
            <div id="exportflight" style="margin-left: 68px;margin-top: -26px; display:none"><img id=" imgexcel" src="../Images/IconExcel.png" style="width:30px;height:30px;cursor: pointer;" title="Export To Excel" onclick="ExportToExcel_CargoClaimReport()" /></div>
        </td>
    </tr>


</table>

<div class="k-grid-header-wrap" id="grid_shipment" style="display:none;">


    <br />
    <table class="appendGrid ui-widget" style="margin-bottom:10px;" align="center" id="tblgrid_shipment">


        <thead class='ui-widget-header' style='text-align:center' id="theadid_shipment">

            <tr>

                <td class="ui-widget-header">Agent</td>
                <td class="ui-widget-header">AWB No.</td>
                <td class="ui-widget-header">Pcs.</td>
                <td class="ui-widget-header">Gross Weight</td>

                <td class="ui-widget-header">
                    Volume
                </td>
                <td class="ui-widget-header">
                    Origin/Destination
                </td>

                <td class="ui-widget-header">
                    Allocation Type
                </td>
                <td class="ui-widget-header">
                    Allocation Code
                </td>
                @*<td class="ui-widget-header">
                        Utilization%
                    </td>*@

            </tr>
        </thead>
        <tbody id="gridbodys_shipment" class="ui-widget-content"></tbody>
    </table>
</div>


<div class="k-grid-header-wrap" id="grid" style="display: none;">
    @*<div class="k-grid-header-wrap" id="grid" style="display: none; overflow-x: scroll; overflow-y: scroll;height:500px;">*@
    <br />
    <table class="appendGrid ui-widget" style="margin-bottom:10px;" align="center" id="tblgrid">


        <thead class="ui-widget-header" style="text-align:center" id="theadid">
            <tr>
                @*<td colspan="2" class="ui-widget-header" rowspan="2">Branch <br />Office</td>*@
                <td class="ui-widget-header" rowspan="2">Branch Office</td>
                <td class="ui-widget-header" rowspan="2">Claimant Name</td>
                <td class="ui-widget-header" rowspan="2">AWB NO.</td>
                <td colspan="2">Flight Date</td>
                <td class="ui-widget-header" rowspan="2">ROUTE</td>
                <td class="ui-widget-header" rowspan="2">Amount Of Claim</td>
                <td colspan="2" class="ui-widget-header">Amount Of Settlement Claim</td>
                <td class="ui-widget-header" rowspan="2">Station Of Irreg</td>

                <td class="ui-widget-header" rowspan="2">Date Claim Letter</td>
                <td colspan="2" class="ui-widget-header">Weight Of Claim </td>
                <td class="ui-widget-header" rowspan="2">Reason Of Claim</td>

                <td class="ui-widget-header" rowspan="2"> Claim Status</td>

                <td class="ui-widget-header" rowspan="2"> Closed Date Final Release</td>

                <td class="ui-widget-header" rowspan="2"> Remarks</td>
            </tr>
            <tr>

                <td class="ui-widget-header">Ex Flight</td>
                <td class="ui-widget-header" style="color:green">D/M/Y</td>
                <td class="ui-widget-header" style="color:Blue">Paid By Insurance</td>
                <td class="ui-widget-header" style="color:Violet ">Paid By Garuda</td>
                <td class="ui-widget-header" style="color:Brown">PCS</td>
                <td class="ui-widget-header" style="color:Brown">KG</td>
            </tr>


        </thead>
        <tbody id="gridbodys" class="ui-widget-content"></tbody>
    </table>
</div>

<script src="~/Scripts/jquery-ui-1.10.2.custom.min.js"></script>

<script src="~/JScript/Irregularity/CargoClaimReport.js"></script>


