<style type="text/css">
    .k-grid-header th.k-header > .k-link {
        cursor: auto;
    }
</style>
<div id="divbody" style="height:auto">
    <table class='WebFormTable' id='tbl' validateonsubmit=true>
        <tr><td class='formSection' colspan='4'>Hold Type Information</td></tr>
        <tr class="DateWiseRow">
            <td class='formlabel' title='From Date'>
                <font color='red'>*</font><span id='spnAirlineCode'>
                    From Date :
                </span>
            </td>
            <td class='formInputcolumn'>
                <input type='text' name='FromDate' id='FromDate' style='width: 133px;' data-valid='required'
                       data-role="datepicker" data-valid-msg='Select From Date' controltype='datetype' value='' placeholder='From Date' />
            </td>
            <td class='formlabel' title='To Date'>
                <font color='red'>*</font><span id='spnAirlineCode'>
                    To Date :
                </span>
            </td>
            <td class='formInputcolumn'>
                <input type='text' name='ToDate' id='ToDate' style='width: 133px;' data-valid='required'
                       data-role="datepicker" data-valid-msg='Select From To Date' controltype='datetype' value='' placeholder='To Date' />
            </td>
        </tr>
        <tr class="DateWiseRow">
            <td class='formlabel' title='Airline'>
                <font color='red'>*</font><span id='spnAirlineCode'>
                    Airline
                </span>
            </td>
            <td class='formInputcolumn'>
                @Html.Hidden("AirlineCode")
                @Html.TextBox("Text_AirlineCode", null, new { data_valid = "required", controltype = "autocomplete", data_role = "autocomplete", data_valid_msg = "Airline Name can not be blank" })
            </td>
            <td class="formlabel"></td>
            <td class="formInputcolumn"></td>
            @*<td class='formlabel' title='HoldType'>
                    Hold Type
            </td>
            <td class='formInputcolumn'>
                @Html.Hidden("HoldType")
                @Html.TextBox("Text_HoldType", null, new { controltype = "autocomplete" })
            </td>*@
        </tr>
        @*<tr class='formSection'>
            <td class='formlabel' title='AWB No'>
                <span id='spnAirlineCode'>
                    AWB No :
                </span>
            </td>
            <td class='formInputcolumn'>
                @Html.Hidden("AWBNo")
                @Html.TextBox("Text_AWBNo", null, new { controltype = "autocomplete", data_role = "autocomplete" })
            </td>
            <td class="formlabel"></td>
            <td class="formInputcolumn">
            </td>
        </tr>*@
        <tr>
            <td class='formlabel'>
                <div id="exportflight" style="float:left;">
                    <img id="imgexcel" src="../Images/IconExcel.png" style="width:30px;height:30px;cursor: pointer" onclick="ExportExcelHoldType()" title="Export To Excel" />
                </div>
            </td>
            <td class='formInputcolumn'>
                <input type='submit' name='operation' onclick="SearchHoldTypeReport()" value='Search' id="btnSearch" class='btn btn-success btnSearch' />
            </td>
            <td class="formlabel"></td>
            <td class="formInputcolumn"></td>
        </tr>
    </table>
    <br />
    <br />
</div>
<table id='tbl'></table>
<div id="example">
    <div id="grid" style="font-size:11px;"></div>
    <script src="~/JScript/Report/HoldTypeReport.js"></script>
</div>


