                            <td class=' formlabel' title='Office'>
                                <font color='red'>*</font><span id='spnAirlineCode'>
                                    To Date :
                                </span>
                            </td>
                            <td class='formInputcolumn'>
                                <input type='text' name='ToDate' id='ToDate' style='width: 133px;' data-valid='required'
                                       data-role="datepicker" data-valid-msg='Select To Date' controltype='datetype' value='' placeholder='To Date' />
                            </td>
                        </tr>



                        <tr>
                            <td class=' formlabel' title='Airlin' colspan="4">
                                <font color='red'></font><span id='spnAirlineCode'>
                                    <div id="exportflight" style="float:left;"><img id="imgexcel" src="../Images/IconExcel.png" style="width:30px;height:30px;cursor: pointer" title="Export To Excel" onclick="ExportToExcel_DailyStockTransactionReport()" /></div>
                                    <center>    <input type='submit' name='operation' onclick="GetReportData()" value='Search' id="btnSearch" class='btn btn-success btnSearch' /></center>
