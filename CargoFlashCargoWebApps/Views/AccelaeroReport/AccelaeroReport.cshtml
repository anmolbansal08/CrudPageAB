<style type="text/css">
    .completeprocess {
        color: #fff;
        background-color: #51a351;
        border-color: #51a351;
        font-size: 10px;
    }

    .incompleteprocess {
        color: #fff;
        background-color: #428bca;
        border-color: #357ebd;
        font-size: 10px;
    }

    .partialprocess {
        color: #fff;
        background-color: #FE9D5A;
        border-color: #FE9D5A;
        font-size: 10px;
    }

    .failureprocess {
        color: #fff;
        background-color: #FE0404;
        border-color: #FE9D5A;
        font-size: 10px;
    }

    .dependentprocess {
        color: #fff;
        background-color: brown;
        border-color: goldenrod;
        font-size: 10px;
    }

    .k-grid-filter {
        display: none;
    }

    .k-link {
        cursor: text;
    }
</style>

@*@model CargoFlash.Cargo.Model.Report.AccelaeroReport*@
<div id="divbody" style="height:auto">



    <table class='WebFormTable' id='tbl' validateonsubmit=true>
        <tr><td class='formSection' colspan='4'>Accelaero Report</td></tr>
        <tr><td class='formActiontitle' colspan='4'><div class='MandatoryTextCss'><font color='red'>*</font> Mandatory Fields</div></td></tr>


        <tr class="DateWiseRow">
            <td class="formlabel"><font color='red'>*</font>From Date</td>
            <td class="formInputcolumn">@Html.TextBox("FromDate", null, new { data_valid = "required", data_valid_msg = "From Date can not be blank" })</td>
            <td class="formlabel"><font color='red'>*</font>To Date</td>
            <td class="formInputcolumn">@Html.TextBox("ToDate", null, new { data_valid = "required", data_valid_msg = "To Date can not be blank" })</td>

        </tr>

        <tr class="DateWiseRow">

            <td class='formlabel' title='Airline'>

                Airline

            </td>
            <td class='formInputcolumn'>
                @Html.Hidden("Airline")
                @Html.TextBox("Text_Airline", null, new { controltype = "autocomplete", data_role = "autocomplete", })
            </td>
            <td class='formlabel' title='Airline'>

                Flight No

            </td>
            <td class='formInputcolumn'>
                @Html.Hidden("FlightNo")
                @Html.TextBox("Text_FlightNo", null, new { controltype = "autocomplete", data_role = "autocomplete", })
            </td>

        </tr>


        <tr class="DateWiseRow">
            <td class="formlabel"><font color='red'></font>Origin</td>
            <td class="formInputcolumn">
                @Html.Hidden("Origin")
                @Html.TextBox("Text_Origin", null, new { controltype = "autocomplete", data_role = "autocomplete" })
            </td>

            <td class="formlabel">Destination</td>
            <td class="formInputcolumn">
                @Html.Hidden("Destination")
                @Html.TextBox("Text_Destination", null, new { controltype = "autocomplete", data_role = "autocomplete" })
            </td>

        </tr>

        <tr class="DateWiseRow">

            <td class='formInputcolumn' colspan="4">
                <input type='submit' onclick="search()" name='operation' value='Search' class='btn btn-success' width="100" />

            </td>
        </tr>

    </table>


</div>
<div id="grid" style="font-size:11px;"></div>
<script src="~/JScript/Report/AccelaeroReport.js"></script>
<script src="~/Scripts/KendoSchedular/Content/JS/jszip.js"></script>