

<div id="divbody" style="height:auto">
    <section id="loginForm">
        <div id='MainDiv'>
            <div id='ApplicationTabs'>
                <div id='ApplicationTabs-1'>
                    <table class='WebFormTable' id='tbl' validateonsubmit=true>
                        <tr><td class='formSection' colspan='4'>CCA Report</td></tr>
                        <tr><td class='formSection' colspan='4'><span id='__SpanHeader__'>CARGO CHARGES CORRECTION ADVICE (CCA)</span><div class='MandatoryTextCss'><font color='red'>*</font> Mandatory Fields</div></td></tr>
                        <tr class="DateWiseRow">
                            <td class=' formlabel' title='Airline'>
                                <font color='red'>*</font><span id='spnAirlineCode'>
                                    Airline
                                </span>
                            </td>
                            <td class='formInputcolumn'>
                                @Html.Hidden("AirlineCode")
                                @Html.TextBox("Text_AirlineCode", null, new { data_valid = "required", controltype = "autocomplete", data_role = "autocomplete", data_valid_msg = "Airline Name can not be blank", style = "width:200px;" })
                            </td>
                            <td class=' formlabel' title='Status'>
                                <font color='red'></font><span id='spnAirlineCode'>
                                    Status :
                                </span>
                            </td>
                            <td class='formInputcolumn'>
                                <input type="radio" name="StatusType" checked="checked" value="0"> Request
                                <input type="radio" name="StatusType" value="1"> Approved
                                <input type="radio" name="StatusType" value="2"> Rejected
                            </td>

                        </tr>
                        <tr class="DateWiseRow">
                            <td class=' formlabel' title='Origin'>
                                <span id='spnAirlineCode'>
                                    Origin :
                                </span>
                            </td>
                            <td class='formInputcolumn'>
                                @Html.Hidden("OriginSNo")
                                @Html.TextBox("Text_OriginSNo", null, new { controltype = "autocomplete", data_role = "autocomplete", style = "width:200px;" })
                            </td>
                            <td class=' formlabel' title='Flight No'>
                                <font color='red'></font><span id='spnAirlineCode'>
                                    Flight No :
                                </span>
                            </td>
                            <td class='formInputcolumn'>
                                @Html.Hidden("FlightSNo")
                                @Html.TextBox("Text_FlightSNo", null, new { controltype = "autocomplete", data_role = "autocomplete" ,style = "width:200px;"  })
                            </td>
                        </tr>
                        <tr>
                            <td class=' formlabel' title='AWB No'>
                                <span id='spnAirlineCode'>
                                    AWB No :
                                </span>
                            </td>
                            <td class='formInputcolumn'>
                                @Html.Hidden("AWBNo")
                                @Html.TextBox("Text_AWBNo", null, new { controltype = "autocomplete", data_role = "autocomplete", style = "width:200px;" })
                            </td>

                            <td class="formlabel" title='CCA No'>
                                CCA No :
                            </td>
                            <td class="formInputcolumn">
                                @Html.Hidden("CCANo")
                                @Html.TextBox("Text_CCANo", null, new { controltype = "autocomplete", data_role = "autocomplete", style = "width:200px;" })
                            </td>
                        </tr>
                        <tr class="DateWiseRow">
                            <td class=' formlabel' title='From Date'>
                                <font color='red'>*</font><span id='spnAirlineCode'>
                                    From Date :
                                </span>
                            </td>
                            <td class='formInputcolumn'>
                                <input type='text' name='FromDate' id='FromDate' style='width: 133px;' data-valid='required'
                                       data-role="datepicker" data-valid-msg='Select From Date' controltype='datetype' value='' placeholder='From Date' />
                            </td>
                            <td class=' formlabel' title='To Date'>
                                <font color='red'>*</font><span id='spnAirlineCode'>
                                    To Date :
                                </span>
                            </td>
                            <td class='formInputcolumn'>
                                <input type='text' name='ToDate' id='ToDate' style='width: 133px;' data-valid='required'
                                       data-role="datepicker" data-valid-msg='Select To Date' controltype='datetype' value='' placeholder='To Date' />
                            </td>
                        </tr>
                        <tr>
                            <td class='formlabel'>
                                <div id="exportflight" style="float:left;"><img id="imgexcel" src="../Images/IconExcel.png" style="width:30px;height:30px;cursor: pointer" title="Export To Excel" onclick="ExportToExcel_CCAReport()" /></div>
                            </td>
                            <td class='formInputcolumn'>
                                <input type='submit' name='operation' onclick="SearchCCAReport()" value='Search' id="btnSearch" class='btn btn-success btnSearch' />
                            </td>
                            <td class=' formlabel'></td>
                            <td class='formInputcolumn'></td>
                        </tr>
                    </table>
                    <br />
                    <br />



                    <div id="BindBlakListTable" style="width:100%">

                    </div>
                </div>

            </div>
        </div>

        <br />
        <br />
        <br />
        <br />
        <div id="example">
            <div id="grid" style="font-size:14px;"></div>

            <script src="~/JScript/CCAReport/CCAReport.js"></script>
        </div>


    </section>


</div>



