

<div id="divbody" style="height:auto">
    <section id="loginForm">
        <div id='MainDiv'>
            <div id='ApplicationTabs'>
                <div id='ApplicationTabs-1'>
                    <table class='WebFormTable' id='tbl' validateonsubmit=true>
                        <tr><td class='formSection' colspan='4'>Marine Insurance BO Report</td></tr>
                        <tr class="DateWiseRow">
                            <td class='formlabel' title='Airline'>
                                <font color='red'>*</font><span id='spnAirline'>
                                    Airline:
                                </span>
                            </td>
                            <td class='formInputcolumn'>
                                @Html.Hidden("Airline")
                                @Html.TextBox("Text_Airline", null, new { data_valid = "required", controltype = "autocomplete", data_role = "autocomplete", data_valid_msg = "Airline can not be blank" })
                            </td>
                            <td class='formlabel' title='City'>
                                <font color='red'></font><span id='spnCity'>
                                    City:
                                </span>
                            </td>
                            <td class='formInputcolumn'>
                                @Html.Hidden("City")
                                @Html.TextBox("Text_City", null, new { controltype = "autocomplete", data_role = "autocomplete" })
                            </td>
                        </tr>
                        <tr class="DateWiseRow">
                            <td class='formlabel' title='From Date'>
                                <font color='red'></font><span id='spnFromDate'>
                                    From Date:
                                </span>
                            </td>
                            <td class='formInputcolumn'>
                                <input type='text' name='FromDate' id='FromDate' style='width: 133px;' data-valid='required'
                                       data-role="datepicker" data-valid-msg='Select From Date' controltype='datetype' value='' placeholder='From Date' />
                            </td>
                            <td class='formlabel' title='To Date'>
                                <font color='red'></font><span id='spnToDate'>
                                    To Date:
                                </span>
                            </td>
                            <td class='formInputcolumn'>
                                <input type='text' name='ToDate' id='ToDate' style='width: 133px;' data-valid='required'
                                       data-role="datepicker" data-valid-msg='Select To Date' controltype='datetype' value='' placeholder='To Date' />
                            </td>
                        </tr>
                        <tr class="DateWiseRow">
                            <td class='formlabel' title='Airline'>
                                <font color='red'></font><span id='spnAirline'>
                                    Agent:
                                </span>
                            </td>
                            <td class='formInputcolumn'>
                                @Html.Hidden("Agent")
                                @Html.TextBox("Text_Agent", null, new { controltype = "autocomplete", data_role = "autocomplete" })
                            </td>
                            <td class='formlabel' title='City'>
                                <font color='red'></font><span id='spnCity'>
                                </span>
                            </td>
                            <td class='formInputcolumn'></td>
                        </tr>
                        <tr>
                            <td class='formlabel'>
                                <form id="MarineToExcel" action="../MarineInsuranceBOReport/ExportToExcel" method="post">
                                    <input id="FromDate" type="hidden" name="FromDate" value="" />
                                    <input id="ToDate" type="hidden" name="ToDate" value="" />
                                    <input id="Airline" type="hidden" name="Airline" value="" />
                                    <input id="City" type="hidden" name="City" value="" />
                                    <input id="Agent" type="hidden" name="Agent" value="" />
                                    <input id="UserSNo" type="hidden" name="UserSNo" value="" />
                                    <div style="float:left; display: block;">
                                        <img id="imgexcel" src="../Images/IconExcel.png" style="width:30px;height:30px;cursor: pointer" title="Export To Excel" onclick="ExportToExcel()" />
                                    </div>
                                </form>
                            </td>
                            <td>
                                @*<input type='submit' name='operation' onclick="SearchMarineInsuranceReport()" value='Search' id="btnSearch" class='btn btn-success btnSearch' />*@
                                <input type="button" name='SearchSave' value='Search' class='btn btn-success' id="btnSubmit" />
                            </td>
                            <td class='formlabel'></td>
                            <td class='formlabel'></td>
                        </tr>
                    </table>
                </div>
            </div>
            <br /><br />

