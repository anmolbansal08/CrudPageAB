                            <td rowspan="2" class="ui-widget-header"><b>ROUTE</b></td>
                            <td rowspan="2" class="ui-widget-header"><b>FLIGHT</b></td>
                            <td rowspan="2" class="ui-widget-header"><b>DATE</b></td>
                            <td rowspan="2" class="ui-widget-header"><b>PCS</b></td>
                            <td rowspan="2" class="ui-widget-header"><b>WEIGHT</b></td>
                            <td rowspan="2" class="ui-widget-header"><b>COMMODITY</b></td>
                            <td rowspan="2" class="ui-widget-header"><b>INSURANCE <br /> COMMODITY <br /> CLASSIFICATION</b></td>
                            <td rowspan="2" class="ui-widget-header"><b>DECLARED <br />VALUE (IDR)</b></td>
                            <td rowspan="2" class="ui-widget-header"><b>CERTIFICATE <br /> NUMBER</b></td>
                            <td colspan="3" class="ui-widget-header"><b>DECLARE VALUE </b></td>
                            <td colspan="2" class="ui-widget-header"><b>NON DECLARE VALUE</b></td>
                            @*<td colspan="2" class="ui-widget-header"><b>DECLARE VALUE</b></td>
                                <td colspan="1" class="ui-widget-header"><b>NON DECLARE VALUE</b></td>
                                <td rowspan="2" class="ui-widget-header"><b>GENERAL<br />  (Declare Value) </b></td>
                                <td rowspan="2" class="ui-widget-header"><b>PREMIUM<br /> (Non Dec.Value)</b></td>*@

                        </tr>
                        <tr>
                            <td class="ui-widget-header"><b>PUBLISH <br />RATE </b></td>
                            <td class="ui-widget-header"><b>CHARGEABLE<br /> RATE </b></td>
                            <td class="ui-widget-header"><b>MINIMUM PREMI<br /> APPLIED </b></td>
                            <td class="ui-widget-header"><b>PUBLISH <br />PREMIUM RATE  </b></td>
                            <td class="ui-widget-header"><b>CHARGEABLE <br />PREMI   </b></td>
                            @*<td class="ui-widget-header"><b>NET <br />PUBLISH RATE  </b></td>
                                <td class="ui-widget-header"><b>NET <br />CHARGEABLE PREMI </b></td>
                                <td class="ui-widget-header"><b>CHARGEABLE <br />PREMI   </b></td>*@

                        </tr>

                    </thead>
                    <tbody id="gridbodys" class="ui-widget-content"></tbody>
                </table>
            </div>
            <div>
                <div id="grid" style="font-size:11px;"></div>
                <script src="~/JScript/Report/MarineInsuranceBOReport.js"></script>
            </div>
        </div>
    </section>
